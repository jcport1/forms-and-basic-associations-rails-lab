<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.label :song_note %>
    <input id="song_notes_1"  name="song[note_contents][]" type="text" value="enter note"/>
    <input id="song_notes_2" name="song[note_contents][]" type="text" value="enter note"/>
  <%= f.submit %>
<% end %>


